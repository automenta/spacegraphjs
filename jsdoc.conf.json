{
    "source": {
        "include": ["spacegraph.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|[\\/\\\\])_"
    },
    "opts": {
        "destination": "./docs/api/",
        "recurse": true,
        "readme": "./README.md"
    },
    "plugins": ["plugins/markdown"]
}
